{
  "path": "cz-conventional-changelog",
  "disableScopeLowerCase": false,
  "disableSubjectLowerCase": false,
  "maxHeaderWidth": 100,
  "maxLineWidth": 100,
  "defaultType": "",
  "defaultScope": "",
  "defaultSubject": "",
  "defaultBody": "",
  "defaultIssues": "",
  "types": {
    "feat": {
      "title": "新增",
      "description": "新功能"
    },
    "fix": {
      "title": "修复",
      "description": "bug 修复"
    },
    "wip": {
      "title": "进行中",
      "description": "功能正在开发中,提交以切换其他分支进行其他开发"
    },
    "style": {
      "title": "UI",
      "description": "UI相关"
    },
    "refactor": {
      "title": "重构",
      "description": "代码重构"
    },
    "docs": {
      "title": "文档",
      "description": "文档更新"
    },
    "build": {
      "title": "构建系统或者包依赖更新",
      "description": "构建系统或者包依赖更新"
    },
    "ci": {
      "title": "CI/CD",
      "description": "CI/CD,自动构建,持续集成相关"
    },
    "test": {
      "title": "测试",
      "description": "测试用例相关"
    },
    "perf": {
      "title": "优化",
      "description": "优化"
    },
    "chore": {
      "title": "非 src 或者 测试文件的更新",
      "description": "非src或者测试文件的更新"
    },
    "revert": {
      "title": "回溯",
      "description": "回到上一个版本"
    }
  }
}
